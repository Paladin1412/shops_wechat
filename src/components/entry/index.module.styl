.reset_btn
  padding: 0
  background-color: transparent
  line-height: 1
  font-size: 24px
  color: #333
  &::after
    display: none
.container
  width:100%
  min-height:132px
  margin-left:auto
  margin-right:auto
  position relative
  display: flex
  flex-direction: column
  align-items: center
  padding: 0 40px
  &:after
    content: attr(data-msg)
    position: absolute
    width:100%
    left: 0
    bottom: 0
    text-align: center
    color:#9A9A9A
    font-size: 28px

.empty_img
  width:100%
  height:auto
.label
  line-height: 50px
  color: #333333
  font-size: 36px
  font-weight: 600
.btn
  width: 204px
  height: 72px
  display: flex
  justify-content: center
  align-items: center
  border: 2px solid #F9482E
  color: #F9482E
  font-size: 28px
  font-weight: 500
  border-radius: 49px
  margin-top: 48px
