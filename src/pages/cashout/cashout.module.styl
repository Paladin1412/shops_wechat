.add_box
  width: 654px
  height: 176px
  display: flex
  flex-direction: column
  justify-content: center
  align-items: center
  background: rgba(245, 245, 245, 1)
  color: rgba(151, 151, 151, 1)
  font-size: 28px
  margin: 32px auto
