page::before
  display: none
.user_box
  // margin-bottom:10px
  width: 100%
  padding: 0 32px
  background: linear-gradient(0deg,rgba(255,169,70,1) 0%,rgba(249,72,46,1) 40%)
  border-radius: 0 0 50% 50% / 0 0 30px 30px
  overflow hidden
.user_box_inner
  display flex
  align-items center
  justify-content space-between
  margin-bottom: 40px

.user_pic
  width 120px
  height 120px
.user_photo
  box-sizing: border-box
  width 120px
  height 120px
  border-radius 50%
  border:4px solid rgba(255,255,255,1)
  background rgba(246, 246, 246, 1)
.name
  font-size 36px
  font-weight 600
  color rgba(255, 255, 255, 1)
  line-height 50px
  margin 0 16px 0 0
  max-width: 300px
  overflow: hidden
  text-overflow:ellipsis
  white-space: nowrap
  text-align: left
.desc
  // margin 15px 0 0 0
  display flex
  align-items center
.id
  font-size 28px
  line-height 40px
  margin 28px 0 0 0
  color rgba(255, 255, 255, 1)
  margin 0 0 6px 0
  text-align: left
.account_info
  position relative
  display: flex
  justify-content: space-between
  align-items: center
  background-color: #ffffff
  padding: 40px 0
.account_item
  position: relative
  flex: 1 1 auto
  display: flex
  flex-direction: column
  align-items: center
  &:not(:nth-child(4))
    &::after
      content ''
      display block
      position absolute
      right: 0
      top: 50%
      transform: translateY(-50%)
      width 1PX
      height: 38px
      background-color: #E8E9EB
.account_type
  font-size 28px
  color #333333
  line-height 40px
.user_info
  margin 0 0 0 24px
  width 80%
  height: 120px
  display: flex
  flex-direction: column
  justify-content: space-between
  .nologinName
    line-height: 120px
    text-align: left
.info
  margin 0 0 0 48px
.price
  height 56px
  font-size 56px
  font-weight bold
  color rgba(255, 255, 255, 1)
  line-height 56px
  letter-spacing 1px
  margin 16px 0 0 0
.btn
  height 68px
  background rgba(255, 255, 255, 1)
  border-radius 37px
  border 4px solid rgba(255, 255, 255, 1)
  text-align center
  line-height 68px
  font-size 28px
  font-weight 500
  color rgba(248, 77, 52, 1)
  padding 0 36px
  display inline-block
.login_btn
  background rgba(248, 78, 53, 1)
  border-radius 42px
  height 80px
  font-size 34px
  font-weight 600
  color rgba(255, 255, 255, 1)
  line-height 80px
  text-align center
  padding 0 36px
  margin 0 0 0 48px
.order_box
  margin: 24px 32px
  padding: 20px 24px 24px
  background: #fff
  border-radius: 12px
  .order_header
    display: flex
    justify-content: space-between
    align-items: center
    margin-bottom: 40px
    .header_title
      font-size: 32px
      color: #333333
      font-weight: 500
      line-height: 45px
    .header_label
      display: flex
      align-items: center
      font-size: 28px
      color: #999999
      line-height: 40px
    .header_arrow
      display inline-block
      margin-left: 20px
      color #ccc
      font-size 16px
.order_list
  width: 100%
  display flex
  justify-content: space-between
  font-size 24px
  color rgba(52, 52, 52, 1)
  text-align center
  padding: 0 24px
  // padding-top 36px
  position relative
  // padding-bottom 36px
  // border-radius: 18px 18px 0 0
.order_item
  flex: 0 0 auto
  position relative
.account_price
  font-size 36px
  font-weight 600
  color #323232
  line-height 43px
.icon
  font-size 48px
  margin-bottom: 10px
.list_box
  padding: 0 24px
  margin: 0 32px 40px
  background: #fff
  border-radius: 12px
.list
  display flex
  justify-content space-between
  align-items: center
  padding 24px 0
  position relative
.list_header
  display: flex
  align-items: center
.list_icon
  width: 44px
  height: 44px
.list_title
  font-size 32px
  font-weight 500
  color #343434
  line-height 45px
  margin-left: 32px
.arrow
  color #333
  font-size 16px
.more
  color rgba(255, 255, 255, 0.9)
  font-size: 30px
.whiteBg
  width: 100%
  height: 30px
  position: absolute
  left: 0
  top: -30px
  z-index: -1
  background-color: #fff
.num
  display flex
  font-size 28px
  font-weight 500
  color rgba(154, 154, 154, 1)
  line-height 40px
.mr
  margin-right 28px
