.order_num
  width 28px
  height 28px
  background rgba(248, 78, 53, 1)
  position absolute
  top 0
  right 0
  transform: translate(50%, -50%)
  font-size 20px
  font-weight bold
  color rgba(255, 255, 255, 1)
  line-height 28px
  text-align center
  border-radius 50%
.levelIcon
  width 37px
  height 28px
  vertical-align middle
.levelIcon_diamond
  width 40px
  height 39px
  vertical-align middle
.levelText
  vertical-align middle
  margin-left 8px
  font-size 28px
  color:rgba(227,228,230,0)
  line-height:28px
  background:linear-gradient(180deg, rgba(238,238,238,1) 0%, rgba(184,184,184,1) 100%)
  -webkit-background-clip:text
  -webkit-text-fill-color:transparent
.goldLevelText
  background:linear-gradient(180deg, rgba(252,234,208,1) 0%, rgba(238,178,102,1) 100%)
  -webkit-background-clip:text
  -webkit-text-fill-color:transparent
.diamondLevelText
  background:linear-gradient(180deg, rgba(245,237,237,1) 0%, rgba(245,244,202,1) 51%, rgba(245,237,237,1) 100%)
  -webkit-background-clip:text
  -webkit-text-fill-color:transparent
.top_back
  background: rgba(247, 248, 250, 1)
  position: fixed
  width: 100%
  height 100%
  top: 0
  left :0
  z-index: -1
.upgrade_container
  width: 100%
  position: relative
  overflow: hidden
  .upgrade_img
    position: absolute
    top: 0
    left: 0
    right: 0
    width: 100%
  .silver_noinvite_img
    height: 149px
  .silver_invited_img
    height: 228px
  .gold_img
    height: 228px
  .diamond_img
    height: 144px
  .upgradeHeader
    position: relative
    padding: 50px 24px 0
    display: flex
    justify-content: space-between
    align-items: center
    .percentInner
      position: absolute
      top: 50px
      height:38px
      min-width: 44px
      display: flex
      justify-content: center
      align-items: center
      color:#CCA574
      font-size: 22px
      padding-bottom: 4px
      .innerBg
        position: absolute
        top: 0
        left: 0
        width: 100%
        height: 100%
    .silverProgress
      width: 420px
      :global
        .at-progress__outer-inner
          background-color: #FBF3CC
    .goldProgress
      width: 420px
      :global
        .at-progress__outer-inner
          background-color: #686463
    .headerTitle
      font-size: 28px
      color: rgba(189, 124, 43, 1)
      font-weight: 500
      line-height: 40px
    .headBtn
      background:rgba(212,160,95,1)
      border-radius:22px
      font-size: 24px
      color: #ffffff
      line-height: 44px
      padding: 0 24px
    .diamondTitle
      color: #FFA946
    .silverBtn
      background-color: rgba(255, 89, 10, 1)
      color: #F7F7F7
    .goldbtn
      background-color: #ffffff
      color: #4B4342
    .diamondbtn
      background-color: #FFA946
      color: #333
  .headerPadding
    padding-top: 80px

  .diamondHeader
    padding-bottom: 26px
  .upgradeFooter
    position: relative
    width: 410px
    margin: 8px 24px 8px
    font-size: 22px
    color: rgba(218, 180, 133, 1)
    line-height: 30px
  .footerMargin
    margin-bottom: 26px
  .goldFooter
    margin-bottom: 26px
    color: #8C8584
.upgrade_tip
  :global
    .at-modal__container
      background:transparent
      width: 620px
  .silverImg
    width:100%
    height:606px
  .img
    width:100%
    height:818px

  .close
    background-color:white
    border-radius: 50%
    width:80px
    height:80px
    font-size: 28px
    text-align: center
    line-height: 84px
    color:#666
    margin-left:auto
    margin-right:auto
.share_popup
  :global
    .layout-body,
    .at-float-layout__container
      min-height: 100px
    .at-float-layout .layout-body
      background:#fff
      min-height:100px
      padding: 0
    .layout-body__content
      min-height: 100px
  .share_title
    position: relative
    width: 100%
    height: 109px
    line-height: 109px
    text-align: center
    font-size: 32px
    color: #333333
  .cancalItem
    width:100%
    display: flex
    justify-content: center
    align-items: center
    margin:30px 0
  .cancalBtn
    width 594px
    height 68px
    background rgba(247, 247, 247, 1)
    border-radius 40px
    display flex
    justify-content center
    align-items center
    color #B3B3B3
    font-size 28px
  .share_container
    overflow hidden
    .item
      flex 1
      text-align center
      padding-top 36px
      text-align: center
      border: 0
      background: #ffffff
      &::after
        display: none
      .font
        color #333
        font-size 28px
        line-height 28px
        margin-bottom 36px
        margin-top 25px
      .imgicon
        width: 100px
        height 100px
        display: block
        margin: 0 auto
.reset_btn
  padding: 0
  background-color: transparent
  line-height: 1
  font-size: 24px
  color: #333
  &::after
    display: none

