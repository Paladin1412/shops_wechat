page 
  background-color: rgba(247, 248, 250, 1)
.container
  padding 8px 32px 0
  background-color: #fff
.list
  display flex
  justify-content space-between
  align-items: center
  position relative
  padding: 32px 0
.cellTitle
  line-height: 45px
  font-size: 32px
  color: rgba(51, 51, 51, 1)
  font-weight: 500
.head_img
  width 120px
  height 120px
  background rgba(246, 246, 246, 1)
  border-radius 50%
  position relative
  overflow hidden
  margin-right: 42px
.user_photo
  width:100%
  height:100%
.upload
  width 540px
  height 540px
  left:-20px
  top:-10px
  position: absolute
  opacity 0
.nickname
  text-align right
  margin-right: 42px
.icon
  position: absolute
  right: 0
  top: 50%
  transform: translateY(-50%)
  font-size: 16px
  color: rgba(51, 51, 51, 1)